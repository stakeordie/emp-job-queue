{
  "name": "@emp/job-evaluator",
  "version": "1.0.0",
  "description": "Service to evaluate and categorize jobs for cleanup monitoring",
  "main": "dist/index.js",
  "bin": {
    "job-evaluator": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/index.js",
    "dev": "tsx src/index.ts",
    "dev:local": "tsx --env-file=.env.secret.local-dev --env-file=.env.local-dev src/index.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@emp/core": "workspace:*",
    "@emp/database": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.0.0"
  },
  "keywords": [
    "job-evaluation",
    "cleanup",
    "monitoring"
  ],
  "author": "EMP Job Queue",
  "license": "MIT"
}