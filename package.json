{
  "name": "emp-redis-js",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript rebuild of emp-redis - Pull-based job broker for AI workloads",
  "type": "module",
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "lint:check": "turbo run lint:check",
    "lint:fix": "turbo run lint:fix",
    "format": "turbo run format",
    "format:check": "turbo run format:check",
    "typecheck": "turbo run typecheck",
    "clean": "turbo run clean",
    "package:worker": "node scripts/package-worker.js",
    "redis:functions:install": "tsx src/cli/redis-functions.ts install",
    "redis:functions:list": "tsx src/cli/redis-functions.ts list",
    "redis:functions:test": "tsx src/cli/redis-functions.ts test",
    "redis:functions:delete": "tsx src/cli/redis-functions.ts delete",
    "redis:deploy:railway": "./scripts/deploy-redis-railway.sh",
    "archive:jobs": "tsx src/cli/archive-jobs.ts",
    "archive:jobs:dry-run": "tsx src/cli/archive-jobs.ts --dry-run",
    "archive:jobs:cleanup": "tsx src/cli/archive-jobs.ts --cleanup",
    "archive:cron": "tsx src/cli/cron-archive.ts",
    "archive:cron:test": "tsx src/cli/cron-archive.ts --verbose",
    "archive:setup-cron": "./scripts/setup-cron.sh",
    "docker:down": "docker compose -f packages/core/src/utils/docker-compose.yml --profile all-workers down",
    "docker:logs": "docker compose -f packages/core/src/utils/docker-compose.yml logs -f",
    "docker:build": "docker compose -f packages/core/src/utils/docker-compose.yml build",
    "docker:up": "docker compose -f packages/core/src/utils/docker-compose.yml up",
    "docker:up:build": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml up --build",
    "docker:one-worker": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile one-worker up",
    "docker:one-worker:build": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile one-worker up --build",
    "docker:multi-sim": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile multi-sim up",
    "docker:multi-sim:build": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile multi-sim up --build",
    "docker:all-workers": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile all-workers up",
    "docker:all-workers:build": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile all-workers up --build",
    "docker:capability-test": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile capability-test up",
    "docker:capability-test:build": "pnpm docker:down && docker compose -f packages/core/src/utils/docker-compose.yml --profile capability-test up --build",
    "machines:base:build": "docker compose -f apps/machines/base_machine/docker-compose.yml build base_machine",
    "machines:base:up": "docker compose -f apps/machines/base_machine/docker-compose.yml up -d base_machine",
    "machines:base:up:build": "docker compose -f apps/machines/base_machine/docker-compose.yml up -d --build base_machine",
    "machines:base:down": "docker compose -f apps/machines/base_machine/docker-compose.yml down",
    "machines:base:logs": "docker compose -f apps/machines/base_machine/docker-compose.yml logs -f base_machine",
    "machines:base:shell": "docker compose -f apps/machines/base_machine/docker-compose.yml exec base_machine /bin/bash",
    "monitor": "pnpm -F monitor dev",
    "docs:dev": "pnpm -F emp-job-queue-docs docs:dev --no-cache",
    "docs:build": "pnpm -F emp-job-queue-docs docs:build --no-cache",
    "docs:preview": "pnpm -F emp-job-queue-docs docs:preview --no-cache",
    "install:clean": "pnpm clean && pnpm install",
    "prebuild": "pnpm format && pnpm lint:check"
  },
  "packageManager": "pnpm@10.10.0",
  "keywords": [
    "job-queue",
    "redis",
    "workers",
    "typescript",
    "ai",
    "comfyui",
    "pull-based",
    "distributed"
  ],
  "author": "EmProps Team",
  "license": "MIT",
  "dependencies": {
    "@types/eventsource": "^3.0.0",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "eventsource": "^4.0.0",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/node-fetch": "^2.6.12",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "ioredis-mock": "^8.9.0",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "majestic": "^1.8.1",
    "node-fetch": "^3.3.2",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.0",
    "tsx": "^4.6.0",
    "turbo": "^2.5.4",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emprops/emp-redis.git",
    "directory": "js-rebuild"
  },
  "bugs": {
    "url": "https://github.com/emprops/emp-redis/issues"
  },
  "homepage": "https://github.com/emprops/emp-redis/tree/main/js-rebuild#readme"
}