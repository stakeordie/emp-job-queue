{
  "name": "emp-redis-js",
  "version": "1.0.0",
  "description": "JavaScript/TypeScript rebuild of emp-redis - Pull-based job broker for AI workloads",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:core": "tsc -p tsconfig.core.json",
    "build:hub": "tsc -p tsconfig.hub.json",
    "build:worker": "tsc -p tsconfig.worker.json",
    "dev": "tsx watch src/index.ts",
    "dev:hub": "tsx watch src/hub/index.ts",
    "dev:worker": "tsx watch src/worker/index.ts",
    "start": "node dist/index.js",
    "start:hub": "node dist/hub/index.js",
    "start:worker": "node dist/worker/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:integration": "jest --testPathPattern=integration",
    "test:unit": "jest --testPathPattern=unit",
    "lint": "eslint 'src/**/*.ts'",
    "lint:check": "eslint 'src/**/*.ts' --max-warnings 250",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "package:hub": "node scripts/package-hub.js",
    "package:worker": "node scripts/package-worker.js",
    "docker:build": "docker-compose build",
    "docker:up": "docker-compose up",
    "docker:down": "docker-compose down",
    "docker:logs": "docker-compose logs -f",
    "clean": "rm -rf dist node_modules",
    "install:clean": "pnpm clean && pnpm install",
    "prebuild": "pnpm format && pnpm lint:check"
  },
  "packageManager": "pnpm@10.10.0",
  "keywords": [
    "job-queue",
    "redis",
    "workers",
    "typescript",
    "ai",
    "comfyui",
    "pull-based",
    "distributed"
  ],
  "author": "EmProps Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emprops/emp-redis.git",
    "directory": "js-rebuild"
  },
  "bugs": {
    "url": "https://github.com/emprops/emp-redis/issues"
  },
  "homepage": "https://github.com/emprops/emp-redis/tree/main/js-rebuild#readme"
}