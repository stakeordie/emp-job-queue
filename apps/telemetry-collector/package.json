{
  "name": "telemetry-collector",
  "version": "1.0.0",
  "description": "Redis Event Stream Collector for Telemetry Events",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "lint": "eslint src/**/*.ts",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:integration": "vitest run --config vitest.config.ts src/__tests__/telemetry-pipeline-integration.test.ts",
    "test:e2e": "vitest run --config vitest.config.ts src/__tests__/*.e2e.test.ts"
  },
  "dependencies": {
    "@emp/core": "workspace:*",
    "ioredis": "^5.3.2",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "@types/node": "^20.8.0",
    "tsx": "^3.14.0",
    "typescript": "^5.2.2",
    "vitest": "^1.0.0"
  }
}