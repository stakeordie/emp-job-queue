{
  "name": "emp-redis-simple-monitor",
  "version": "1.0.0",
  "description": "Simple Redis Monitor for emp-redis JavaScript - Standalone monitoring dashboard",
  "type": "module",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/server.ts",
    "start": "node dist/server.js",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist"
  },
  "keywords": [
