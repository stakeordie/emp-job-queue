{
  "name": "worker",
  "version": "1.0.0",
  "description": "TypeScript Redis-direct workers for emp-job-queue",
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "emp-worker": "dist/redis-direct-worker.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "mkdir -p ../../logs && echo '   - Logs:         pnpm logs:worker' && echo '   - All Logs:     pnpm logs:all' && tsx watch src/redis-direct-worker.ts 2>&1 | tee ../../logs/worker.log",
    "start": "node dist/redis-direct-worker.js",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts'",
    "lint:check": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --max-warnings 50",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@emp/core": "workspace:*",
    "axios": "^1.6.2",
    "ws": "^8.14.2",
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/ws": "^8.5.9",
    "@types/express": "^4.17.21",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.2.2",
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "@types/jest": "^29.5.8",
    "tsx": "^4.6.0"
  }
}