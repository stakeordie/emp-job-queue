{
  "name": "worker",
  "version": "1.0.0",
  "description": "TypeScript Redis-direct workers for emp-ai-backend",
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "emp-worker": "dist/redis-direct-worker.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "mkdir -p ../../logs && echo '   - Logs:         pnpm logs:worker' && echo '   - All Logs:     pnpm logs:all' && tsx watch src/redis-direct-worker.ts 2>&1 | tee ../../logs/worker.log",
    "dev:ollama": "mkdir -p ../../logs && echo '   - Logs:         pnpm logs:ollama' && echo '   - All Logs:     pnpm logs:all' && tsx watch src/ollama-worker.ts 2>&1 | tee ../../logs/ollama-worker.log",
    "start": "node dist/redis-direct-worker.js",
    "start:ollama": "node dist/ollama-worker.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts'",
    "lint:check": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --max-warnings 50",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@azure/storage-blob": "^12.27.0",
    "@emp/core": "workspace:*",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.62.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.54.2",
    "@opentelemetry/resources": "^1.28.0",
    "@opentelemetry/semantic-conventions": "^1.28.0",
    "axios": "^1.6.2",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "node-fetch": "^3.3.2",
    "openai": "^5.10.2",
    "uuid": "^9.0.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "ws": "^8.14.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.54.0",
    "nock": "^14.0.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}