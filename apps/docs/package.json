{
  "name": "docs",
  "version": "1.0.0",
  "description": "EmProps Job Queue Documentation using VitePress",
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "scripts": {
    "dev": "mkdir -p ../../logs && echo '   - Logs:         pnpm logs:docs' && echo '   - All Logs:     pnpm logs:all' && vitepress dev src 2>&1 | tee ../../logs/docs.log",
    "build": "vitepress build src",
    "preview": "vitepress preview src"
  },
  "dependencies": {
    "@braintree/sanitize-url": "^6.0.2",
    "cytoscape": "^3.25.0",
    "cytoscape-cose-bilkent": "^4.1.0",
    "dayjs": "^1.11.8",
    "khroma": "^2.0.0",
    "mermaid": "^10.2.2",
    "panzoom": "^9.4.3",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/node": "^22.10.7",
    "typescript": "^5.3.3",
    "vitepress": "^1.6.3",
    "vitepress-plugin-mermaid": "^2.0.10"
  }
}