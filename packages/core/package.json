{
  "name": "@emp/core",
  "version": "1.0.0",
  "description": "Core types, Redis functions, and utilities for emp-ai-backend",
  "type": "module",
  "packageManager": "pnpm@10.12.4",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/types-only.d.ts",
      "import": "./dist/types-only.js"
    }
  },
  "scripts": {
    "build": "tsc && node ../../scripts/generate-build-metadata.js @emp/core .",
    "dev": "tsc --watch",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts'",
    "lint:check": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --max-warnings 50",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "@types/eventsource": "^3.0.0",
    "@types/node-fetch": "^2.6.12",
    "axios": "^1.6.2",
    "commander": "^11.1.0",
    "eventsource": "^4.0.0",
    "ioredis": "^5.3.2",
    "node-fetch": "^2.7.0",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "winston-transport": "^4.9.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "@vitest/ui": "^3.2.4",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsx": "^4.6.0",
    "typescript": "^5.2.2",
    "vitest": "^3.2.4"
  }
}