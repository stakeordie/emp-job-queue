{
  "name": "@emp/core",
  "version": "1.0.0",
  "description": "Core types, Redis functions, and utilities for emp-job-queue",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && mkdir -p dist/utils && cp src/utils/docker-compose.yml dist/utils/",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts'",
    "lint:check": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --max-warnings 50",
    "lint:fix": "eslint 'src/**/*.ts' --ignore-pattern '**/*.test.ts' --fix",
    "format": "prettier --write 'src/**/*.ts'",
    "format:check": "prettier --check 'src/**/*.ts'",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "dependencies": {
    "ioredis": "^5.3.2",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.14.2",
    "zod": "^3.22.4",
    "commander": "^11.1.0",
    "axios": "^1.6.2",
    "@types/eventsource": "^3.0.0",
    "eventsource": "^4.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.9.0",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.5.9",
    "typescript": "^5.2.2",
    "prettier": "^3.1.0",
    "eslint": "^8.54.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.0",
    "@types/jest": "^29.5.8",
    "tsx": "^4.6.0"
  }
}