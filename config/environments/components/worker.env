# Redis component configurations
NAMESPACE=WORKER

[default]
MAX_CONCURRENT_JOBS=100
UNIFIED_MACHINE_STATUS=true

[local]
UNIFIED_MACHINE_STATUS=true
BUNDLE_MODE=local

[production]
UNIFIED_MACHINE_STATUS=true
BUNDLE_MODE=remote

[prod-test]
UNIFIED_MACHINE_STATUS=true
BUNDLE_MODE=local
MAX_CONCURRENT_JOBS=10
LOG_LEVEL=debug
POLL_INTERVAL_MS=500
HEARTBEAT_INTERVAL_MS=15000