NAMESPACE=MACHINE

[default]
HEALTH_PORT=9090
# Port offset for multiple local machines - set MACHINE_INSTANCE=0,1,2,etc
INSTANCE=0
ENV_ENCRYPTION_KEY=${ENV_ENCRYPTION_KEY}

[sim]

[local]
# For local development with multiple machines, set unique instances
# Machine 0: 9090, Machine 1: 9100, Machine 2: 9110, etc.
# Disable all ports by default - use --open at runtime to expose specific ports
# DISABLE_PORTS=true
GPU_MODE=mock
LOG_LEVEL=debug

[production]
EXPOSED_PORTS=9090:9090
GPU_MODE=actual
LOG_LEVEL=info