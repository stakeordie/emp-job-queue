# EmProps Job Queue - Secrets Template
# Copy this to .env.secrets.local and fill in your actual values
# This file contains sensitive configuration that should not be committed to git

# Storage Provider (AWS/Azure)
AWS_ACCESS_KEY_ID=your_aws_access_key_here
AWS_SECRET_ACCESS_KEY_ENCODED=your_encoded_aws_secret_key_here
AWS_DEFAULT_REGION=us-west-2
AZURE_STORAGE_ACCOUNT=your_azure_storage_account
AZURE_STORAGE_KEY=your_azure_storage_key
CLOUD_PROVIDER=aws
CLOUD_STORAGE_PROVIDER=aws
CLOUD_STORAGE_CONTAINER=your_storage_container
CLOUD_CDN_URL=https://your-cdn-url.com

# API Tokens
HF_TOKEN=your_huggingface_token
CIVITAI_TOKEN=your_civitai_token
OPENAI_API_KEY=your_openai_api_key

# Ollama Configuration
OLLAMA_HOST=localhost
OLLAMA_PORT=11434
OLLAMA_DEFAULT_MODEL=llama2

# EmProps Configuration
EMPROPS_DEBUG_LOGGING=false
EMPROPS_API_AUTH=your_emprops_api_auth_token
EMPROPS_API_KEY=your_emprops_api_key

# Worker ComfyUI Remote Configuration
WORKER_COMFYUI_REMOTE_HOST=localhost
WORKER_COMFYUI_REMOTE_PORT=8188
WORKER_COMFYUI_REMOTE_USERNAME=your_comfyui_username
WORKER_COMFYUI_REMOTE_PASSWORD=your_comfyui_password

# Authentication
AUTH_TOKEN=your_auth_token_here

# Dash0 Telemetry (Observability)
DASH0_API_KEY=your_dash0_api_key
DASH0_ENDPOINT=https://ingress.us-west-2.aws.dash0.com/v1/metrics
DASH0_TRACES_ENDPOINT=https://ingress.us-west-2.aws.dash0.com:4317
DASH0_METRICS_ENDPOINT=https://ingress.us-west-2.aws.dash0.com:4317
DASH0_LOGS_ENDPOINT=https://ingress.us-west-2.aws.dash0.com/logs/json

# Machine Environment Encryption
ENV_ENCRYPT_KEY=your_32_character_encryption_key_here