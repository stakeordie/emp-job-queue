# Environment secrets template
# Copy this file to .env.secrets.local and update with real values
# This file contains sensitive information and should not be committed to git

# Development environment secrets
DEV_REDIS_PASSWORD=your-dev-redis-password

# Staging environment secrets  
STAGING_REDIS_PASSWORD=your-staging-redis-password

# Production environment secrets
PROD_REDIS_PASSWORD=your-prod-redis-password

# AWS credentials
AWS_ACCESS_KEY_ID=your-aws-access-key-id
AWS_SECRET_ACCESS_KEY_ENCODED=your-encoded-aws-secret-key

# Azure credentials (if using Azure)
AZURE_STORAGE_ACCOUNT=your-storage-account
AZURE_STORAGE_KEY=your-storage-key

# Remote ComfyUI Server credentials
REMOTE_COMFYUI_HOST=your-comfyui-server.com
REMOTE_COMFYUI_PORT=8188
REMOTE_COMFYUI_USERNAME=your-comfyui-username
REMOTE_COMFYUI_PASSWORD=your-comfyui-password